{
  "name": "backend-main-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/main-api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/main-api/test/jest-e2e.json",
    "prepare": "husky install",
    "typeorm": "typeorm-ts-node-commonjs",
    "create:migration": "typeorm migration:create apps/main-api/src/migrations -n",
    "migrate": "npx typeorm-ts-node-commonjs migration:run -d apps/main-api/typeorm.config.ts",
    "downgrade": "npx typeorm-ts-node-commonjs migration:revert -d apps/main-api/typeorm.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.627.0",
    "@aws-sdk/s3-request-presigner": "^3.627.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@css-inline/css-inline-linux-arm64-musl": "0.14.1",
    "@google/generative-ai": "^0.21.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^10.2.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@novu/node": "^2.0.1",
    "@payos/node": "^1.0.10",
    "@redis/client": "^1.6.0",
    "@types/lodash": "^4.17.7",
    "@types/otp-generator": "^4.0.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.3",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.34.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "diff-match-patch": "^1.0.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "firebase-admin": "^12.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "handlebars": "^4.7.8",
    "husky": "^9.1.4",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.46",
    "nest-winston": "^1.9.7",
    "nestjs-s3": "^2.0.1",
    "nodemailer": "^6.9.14",
    "otp-generator": "^4.0.1",
    "passport-custom": "^1.1.1",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "serve-static": "^1.16.2",
    "tmp": "^0.2.3",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/types(|/.*)$": "<rootDir>/libs/types/src/$1",
      "^@app/dtos(|/.*)$": "<rootDir>/libs/dtos/src/$1",
      "^@app/constants(|/.*)$": "<rootDir>/libs/constants/src/$1",
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@app/shared-modules(|/.*)$": "<rootDir>/libs/shared-modules/src/$1",
      "apps/(.*)": "<rootDir>/apps/$1"
    }
  }
}
